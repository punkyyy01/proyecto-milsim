{
  "version": 2,
  "builds": [
    {
      "src": "gestion_milsim/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "gestion_milsim/wsgi.py"
    }
  ]
}
